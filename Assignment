int[] arr=new int[3];
arr[0]=10;
arr[1]=20;
arr[2]=30;
for(int i=1;i<arr.length;i++){
System.out.println("element of index"+i+a[i]);
}
}}


//java program for narrowing casting//

//solution//
public class Main{
public static void main(String args[]){
double myDouble=9.78;
int myInt=(int) myDouble;
System.out.println(myDouble);
System.out.println(myInt);
}}


//display the data of student//

//solution//
public class Main{
public static void main(String args[]){
int rollno;
String name;
double marks;
rollno=15;
name="vanshita";
marks=89.5;
System.out.println("your name is:"+name);
System.out.println("your roll number is:"+rollno);
System.oit.println("your marks is:"+marks);
}}


// swap two numbers//

//solution//
public class Main{
public static void main(String args[]){
int a=10;
int b=20;
int temp;
System.out.println("before swapping a:"+a+" and b:"+b);
temp=a;
a=b;
b=temp;
System.out.println("after swapping a:"+a+" and b"+b);
}}


//java program for widening casting//

//solution//
public class Main{
public static void main(String args[]){
int myInt=15;
double myDouble=myInt;
System.out.println(myInt);
System.out.println(myDouble);
}
}

